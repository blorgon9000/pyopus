* Downloaded from http://www.eas.asu.edu/~ptm/
* # 180nm BSIM3 model card for bulk CMOS: V0.0

* changed lmax to 4u
* parametrized vth0 and u0

* WCD model 
.lib wcd
.lib 'cmos180n.lib' dfl
.param vth0n=vth0nt
.param u0n=u0nt
.param vth0p=vth0pt
.param u0p=u0pt
.lib 'cmos180n.lib' model
.endl

* Typical mean
.lib tm
.lib 'cmos180n.lib' dfl
.param vth0n=vth0nt
.param u0n=u0nt
.param vth0p=vth0pt
.param u0p=u0pt
.lib 'cmos180n.lib' model
.endl

* Worst power
.lib wp
.lib 'cmos180n.lib' dfl
.param vth0n=vth0nt*(1-vth0nd)
.param u0n=u0nt*(1+u0nd)
.param vth0p=vth0pt*(1-vth0pd)
.param u0p=u0pt*(1+u0pd)
.lib 'cmos180n.lib' model
.endl

* Worst speed
.lib ws
.lib 'cmos180n.lib' dfl
.param vth0n=vth0nt*(1+vth0nd)
.param u0n=u0nt*(1-u0nd)
.param vth0p=vth0pt*(1+vth0pd)
.param u0p=u0pt*(1-u0pd)
.lib 'cmos180n.lib' model
.endl

* Worst one
.lib wo
.lib 'cmos180n.lib' dfl
.param vth0n=vth0nt*(1-vth0nd)
.param u0n=u0nt*(1+u0nd)
.param vth0p=vth0pt*(1+vth0pd)
.param u0p=u0pt*(1-u0pd)
.lib 'cmos180n.lib' model
.endl

* Worst zero
.lib wz
.lib 'cmos180n.lib' dfl
.param vth0n=vth0nt*(1+vth0nd)
.param u0n=u0nt*(1-u0nd)
.param vth0p=vth0pt*(1-vth0pd)
.param u0p=u0pt*(1+u0pd)
.lib 'cmos180n.lib' model
.endl


********************
